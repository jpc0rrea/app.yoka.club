{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "next": "next dev",
    "dev": "kill-port 3000 && npm run services:up && npm run migration:run && npm run next && npm run services:stop",
    "postinstall": "prisma generate",
    "lint": "npm run lint:next && npm run lint:prettier",
    "start": "next start",
    "services:up": "docker-compose -f src/infra/docker-compose.development.yml up -d || docker compose -f src/infra/docker-compose.development.yml up -d",
    "services:stop": "docker-compose -f src/infra/docker-compose.development.yml stop || docker compose -f src/infra/docker-compose.development.yml stop",
    "services:down": "docker-compose -f src/infra/docker-compose.development.yml down || docker compose -f src/infra/docker-compose.development.yml down",
    "migration:run": "npx prisma db push",
    "migration:seed": "ts-node -r dotenv-expand/config -r tsconfig-paths/register --project tsconfig.json src/infra/scripts/seed-database.ts"
  },
